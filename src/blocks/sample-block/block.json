{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "boilerplate-blocks/sample-block",
	"version": "0.1.0",
	"title": "Boilerplate Blocks",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"content": {
			"type": "string"
		},
		"tag": {
			"type": "string",
			"default": "h2"
		},
		"textColor": {
			"type": "string"
		},
		"fontSize": {
			"type": "string"
		},
		"alignmentDesktop": {
			"type": "string"
		},
		"alignmentTablet": {
			"type": "string"
		},
		"alignmentMobile": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "boilerplate-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}